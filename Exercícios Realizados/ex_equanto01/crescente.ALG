Algoritmo "crescente"



Var

   x, y: inteiro

Inicio

   escreval("Escreva dois números: ")
   leia(x)
   leia(y)
   
   enquanto x <> y faca
   
      se x < y entao
         escreval("CRESCENTE!")
      senao
           escreval("DECRESCENTE!")
      fimse

      escreval("Escreva outros dois números: ")
      leia(x)
      leia(y)

       
   fimenquanto



Fimalgoritmo